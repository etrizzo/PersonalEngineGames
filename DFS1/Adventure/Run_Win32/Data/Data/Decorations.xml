<DecorationDefinitions>

  <DecorationDefinition name="GreenTree">
    <Size physicsRadius=".15" drawRadius="1.9" aspect=".8" pivot="0.51,0.2"/>
    <SpriteAnimSet spriteSheet="DECORATIONS/green_trees.png" spriteLayout="2,1">
      <SpriteAnim name="Idle" spriteIndexes="0~1" type="oneInRange"/>
    </SpriteAnimSet>
    <Collision
		  allowsWalking="false"
		  allowsFlying="true"
		  allowsSwimming="false"
	 />
  </DecorationDefinition>
  <DecorationDefinition name="DeadTree">
    <Size physicsRadius=".15" drawRadius="1.9" aspect=".6" pivot="0.5,0.05"/>
    <SpriteAnimSet spriteSheet="DECORATIONS/dead_tree.png" spriteLayout="1,1">
      <SpriteAnim name="Idle" spriteIndexes="0"/>
    </SpriteAnimSet>
    <Collision
      allowsWalking="false"
      allowsFlying="true"
      allowsSwimming="false"
   />
  </DecorationDefinition>
</DecorationDefinitions>
